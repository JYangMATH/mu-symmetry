<Group labelreference="L304" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">SeekProjBase := proc(delta, mu)
  local n,m,p,P,ps, esp, rv, f,F, Fs,i,j;

  n   := add(mu);
  m   := nops(mu):
  if delta&gt;n then 
    P := partition(delta, n);
  else 
    P := partition(delta);
  fi;

  esp := genelsym(n);
  rv  := {seq(seq(z[j]=r[i], j=add(mu[i], i=1..i-1)+1..add(mu[i], i=1..i)), i=1..m)};
  F   := []; ps := []:
  for p in P do
    f := subs(rv, product(esp[p[i]], i=1..nops(p))); 
    if CheckPolyLD([op(F), f])=0 then 
      F  := [op(F), f]:
      ps := [op(ps), p]; 
    fi;
  od:

  Fs := []:
  for p in ps do
    Fs  := [op(Fs), product(sigma[p[i]], i=1..nops(p))];
  od;

  return Fs, nops(P), nops(Fs);
end:

CheckPolyLD := proc(P)
  local var, t, F, Fs, C, sol;

  var := indets(P);
  t   := nops(P):
  F   := sum(k[i]*P[i], i=1..t):
  Fs  := sum(k[i]*p[i], i=1..t);
  C := {coeffs(collect(F, var, 'distributed'), [op(var)])};
  
  sol := solve(C):
  if subs(sol, Fs)=0 then return 0; else return 1; fi;
end:


CheckLD := proc(delta, mu)
  local n,m,p,P,esp, N, F, G, Fs, C, sol, i;

  n   := add(mu);
  m   := nops(mu):
  P   := partition(delta, n);
  esp := Specialization(genelsym(n), mu);
  N   := 0;F := 0; Fs := 0:
  for p in P do
    N   := N+1; 
    F   := F + k[N]*product(esp[p[i]], i=1..nops(p));
    Fs  := Fs +k[N]*product(sigma[p[i]], i=1..nops(p)); 
  od:

  C := {coeffs(collect(F, {seq(r[i], i=1..m)}, 'distributed'), [seq(r[i], i=1..m)])};
  
  sol := solve(C):
  return subs(sol, Fs);
  
end:</Text-field>
</Input>
