<Section collapsed="true" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 2" layout="Heading 2">Generate mu-gist by linear system solving</Text-field></Title>
<Group hide-input="false" labelreference="L334" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">LSMgist := proc(F, mu)
##############################
#
# Input:  F, a mu-symmetric polynomial; mu, multiplicity structure
# Output: convert F into an expression in z[0], ...,z[n-1] by solving linear systems
#
##############################
  local n, m, delta, B, N, i, G, H, C, kv, ind, K, M;
 
  n   := add(mu);
  m   := nops(mu):
delta := degree(F, {seq(r[i], i=1..m)});
  M   := MonomialBasis(delta, n);
  B   := Specialization(M, mu);
  N   := nops(B);
  G   := sum(k[i]*z[i], i=1..N);
  H   := subs({seq(z[i]=B[i], i=1..N)}, G);
  C   := {coeffs(collect(H-F, {seq(r[i], i=1..m)}, 'distributed'), [seq(r[i], i=1..m)])};
  kv  := solve(C);

  if nops({kv})=0 then 
    return 0; 
  else 
    ind := indets({seq(rhs(kv[i]), i=1..N)});
    G := subs({seq(ind[i]=0, i=1..nops(ind))}, subs(kv, G));
#    M := Ps2Ep(M):
#    G := subs({seq(z[i]=M[i], i=1..N)}, G):
    return expand(G);
  fi;
  
end:

MonomialBasis := proc(delta, n)
  local P, pm, pt, p,pc,q,i,Q;

  P := partition(delta);
  Q := []:
  
  for p in P do
    pc := nops(p):
    if pc&lt;n+1 then
      pt := [seq(0, i=1..n-pc), op(p)];
      pm := permute(pt):
      Q   := [op(Q),add(product(x[i]^q[i], i=1..n), q in pm)];
    fi;
  od:

  return Q;
end:</Text-field>
</Input>
</Group>
