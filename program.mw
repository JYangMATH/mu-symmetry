<Group labelreference="L305" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">mucondition := proc(mu)
##############################
#
# Input:  multiplicity structure mu
# Output: conditions in a[0], ...,a[n-1] for f having multiplicity structure mu
#
##############################
  local n, f, m, Q, B, i, k, C, sigma;

  n   := add(mu);
  f   := x^n+sum((-1)^i*sigma[i]*x^(n-i), i=1..n);
  m   := nops(mu):
  Q   := [coeffs(collect(f-product((x-r[i])^(mu[i]), i=1..m), x), x)]:
  B   := Basis(Q, plex(seq(r[i], i=1..m),seq(sigma[i], i=1..n)));
  
  C  := []:
  for k from 1 to nops(B) do
    if degree(B[k], {seq(r[i], i=1..m)})=0 then C := [op(C), B[k]]; fi;
  od:

  return C:
end:</Text-field>
</Input>
