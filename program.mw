elsym := proc(g,n)
##############################
#
# Input:  a symmetric polynomial g w.r.t. x[1],...,x[n]
# Output: expression of g in a[0],...,a[n-1]
#
##############################

  local h,f,v,r,ht,gs,i;
  h := convert(g,elsymfun);
  f := expand(mul(X+x[i],i=1..n));
  v := {seq(coeff(f,X,n-i)=e[i],i=1..n)};
  h := eval(h,v);
  r := subs({seq(e[i]=0, i=1..n)}, h);
  if r=0 then 
     gs := eval(h,x[1]=e[n]/mul(x[i],i=2..n)); 
  else
    ht := h-r;
    ht := eval(ht,x[1]=e[n]/mul(x[i],i=2..n)); 
    gs := ht;
    r  := lcoeff(r)*eval(factor(r)/lcoeff(r), v):
    if degree(r, {seq(x[i], i=1..n)})=0 then
      gs := gs+r;
    else
      gs := gs+eval(r,x[1]=e[n]/mul(x[i],i=2..n)); 
    fi;
  fi;

    return gs;
end:


genelsym := proc(n)
##############################
#
# Input:  degree n
# Output: the n elementary symmetric polynomial in x[1],...,x[n]
#
##############################
  local i, Q, f;

  Q := []:
  f := collect(product(X+x[i], i=1..n)-X^n, X):
  for i from 1 to n do
    Q := [op(Q), coeff(f, X, n-i)]:
  od:

  return expand(Q);
end:


PowerSumBasis := proc(n)
  local i,j;
  return [seq(sum(x[j]^i, j=1..n), i=1..n)]:
end:


CompleteHomoBasis := proc(n)
  local P, pm, pt, p,pc,q,qv, delta,Q,i;

  Q  := []:
  for delta from 1 to n do
    P  := partition(delta);
    qv := 0;
    for p in P do
      pc := nops(p):
      if pc&lt;n+1 then
        pt := [seq(0, i=1..n-pc), op(p)];
        pm := permute(pt):
        qv := qv+add(product(x[i]^q[i], i=1..n), q in pm);
      fi;
    od:
    Q := [op(Q), qv];
  od:

  return Q;
end:


Sp2Ep := proc(n)
  local P, i;

  P := [e[1]]:
  for i from 2 to n do
    P := [op(P), expand(sum((-1)^(j-1)*e[j]*P[i-j], j=1..i-1)+(-1)^(i-1)*i*e[i])]:
  od:

  return P;
end:


Hp2Ep := proc(n)
  local P, i;

  P := [e[1]]:
  for i from 2 to n do
    P := [op(P), expand(sum((-1)^(j-1)*e[j]*P[i-j], j=1..i-1)+(-1)^(i-1)*e[i])]:
  od:

  return P;
end:


Ps2Ep := proc(Ps)
  local n,p,i,Q;

  n := nops(indets(Ps)):
  Q := []:
  for i from 1 to nops(Ps) do
    Q := [op(Q), elsym(Ps[i], n)];
  od;

  return Q;
end:


Specialization := proc(M,mu)
  local m, av, N, i, j;

  m := nops(mu):
  av := {};
  N := 1;  

  for i from 1 to m do
    av := {op(av), seq(x[j]=r[i], j=N..N+mu[i]-1)};
    N  := N+mu[i];
  od:

  return subs(av, M);
end:


Dplus := proc(mu)
##############################
#
# Input:  multiplicity structure mu
# Output: compute D+(mu) in a[0], ...,a[n-1] by solving linear equation systems
#
##############################

  local Dp, i, j, m;

  m   := nops(mu);
  Dp  := product(product((r[i]-r[j])^(mu[i]+mu[j]), j=i+1..m), i=1..m);
  return Dp;

end:

wdeg := proc(f,z)
#   Compute the weighted degree of a polynomial f in z
  local i, n,F,wd,T,d;
  n  := nops(z);
  F  := f;
  wd := 0;
  while F&lt;&gt;0 do
    T := LeadingMonomial(F, plex(op(z)));
    d := wdeg4term(T,z,n);
    if d&gt;wd then wd := d; fi;
    F := F-T*LeadingCoefficient(F, plex(op(z)));
  od;
  return wd;
end:

wdeg4term := proc(t,z,n)
  local i;
  return add(i*degree(t,z[i]), i=1..n);
end:</Text-field>
