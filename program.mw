<Section collapsed="true" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 2" layout="Heading 2">3.2 Generate mu-gist with canonize+reduce</Text-field></Title>
<Group labelreference="L364" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">CREgist := proc(F, mu)
##############################
#
# Input:  F, a mu-symmetric polynomial; mu, multiplicity structure
# Output: convert F into an expression in z[0], ...,z[n-1] by reduction
#
##############################

  local m, delta, n, e, P,p,Bo,B,C,V,R,v,i,t1,t2,t3;

t1 := time():
  m   := nops(mu):
delta := degree(F, {seq(r[i], i=1..m)}):
  n   := add(mu);
  e   := Specialization(genelsym(n), mu);

  if delta&gt;=n then 
    P := partition(delta,n);
  else
    P := partition(delta);
  fi;
  Bo := []:
  for p in P do
    Bo := [op(Bo), product(z[p[i]], i=1..nops(p))];
  od:
  
  B := expand(subs({seq(z[i]=e[i], i=1..n)}, Bo)):
  B,C := MCanonize(B,Bo);
t2 := time():
  R,v := MmuReduce(F, B);
t3 := time():
  if R=0 then 
    return expand(sum(v[i]*C[i], i=1..nops(v))), [t2-t1, t3-t2]; 
  else 
    return 0, [t2-t1, t3-t2]; 
  fi;
end:


MmuReduce := proc(F, B)
# Compute the reduced polynomial of F and the &quot;quotients&quot; w.r.t. B 
  local m, l, i, k, q, f, lf, lb, v;
  
  m  := nops(indets(B)): 
  l  := nops(B);
  i  := l;
  q  := 0:
  f  := expand(F):
  v  := []:

  while f&lt;&gt;0 and i&gt;0 do
    lf := LeadingTerm(f, plex(seq(r[m-k+1], k=1..m)));
    lb := LeadingTerm(B[i], plex(seq(r[m-k+1], k=1..m)));
    if lf[2]=lb[2] then
      f := expand(f-lf[1]/lb[1]*B[i]);
      i := i-1;
      v := [lf[1]/lb[1], op(v)];
    elif LeadingTerm(lf[2]-lb[2], plex(seq(r[m-k+1], k=1..m)))=lf[2] then
      q := q + lf[1]*lf[2]:
      f := f - lf[1]*lf[2]:
    else
      i := i-1;
      v := [0, op(v)];
    fi;
  od:
  if nops(v)&lt;l then v := [seq(0, i=1..l-nops(v)), op(v)]: fi;
  
  return q+f, v;
end:

MCanonize := proc(Fs, Fo)
# Generate the canonical set of Fs

  local B, C, N, i, b, v;

  B := []; C := []:
  N := nops(Fo);
  for i from N to 1 by -1 do
    b   := Fs[i];
    b,v := MmuReduce(b, B);
    if b&lt;&gt;0 then 
      B,C := MpolyInsert(b, B, v, C, i, Fo):
    fi;
  od;

  return B, C;
end:

MpolyInsert := proc(b, B, v, C, iv, Fo)

  local m,p,lB,L,k,c,i:

  m  := nops(indets([b, op(B)]));
  p  := LeadingMonomial(b, plex(seq(r[m-k+1], k=1..m)));
  lB := LeadingMonomial(B, plex(seq(r[m-k+1], k=1..m)));
  L  := nops(B):
  if L=0 then return [b],[Fo[iv]]; fi;

  c := Fo[iv]-sum(v[i]*C[i], i=1..L);
  for k from 1 to L do
    if LeadingMonomial(p-lB[k], plex(seq(r[m-i+1], i=1..m))) = lB[k] then
      return [op(1..k-1, B), b, op(k..L, B)], [op(1..k-1, C), c, op(k..L, C)]; 
    else 
      if k=L then return [op(B), b], [op(C), c]; fi;
    fi;
  od;
end:</Text-field>
</Input>
</Group>
