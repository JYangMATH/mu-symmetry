<Section collapsed="true" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 2" layout="Heading 2">2. Generate mu-gist by linear system solving</Text-field></Title><Presentation-Block>
<Group view="presentation" hide-input="false" hide-output="true" inline-output="false" labelreference="L184" drawlabel="true">
</Group>
<Group view="code" hide-input="false" labelreference="L186" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">LSEgist := proc(F, mu)
##############################
#
# Input:  F, a mu-symmetric polynomial; mu, multiplicity structure
# Output: convert F into an expression in z[0], ...,z[n-1] by solving linear systems
#
##############################
  local n, m, delta, P, p, e, N, i,j, G,H, C, sol, ind;
 
  n   := add(mu);
  m   := nops(mu):
delta := degree(F, {seq(r[i], i=1..m)});
  if delta&gt;=n then
    P := partition(delta, n);
  else
    P := partition(delta);
  fi;
  e := Specialization(genelsym(n), mu);

  N := 0;G := 0: 
  for p in P do
    N  := N+1; 
    G  := G + k[N]*product(z[p[i]], i=1..nops(p));
  od:
  H  := subs({seq(z[i]=e[i], i=1..n)}, G);

  C  := {coeffs(collect(H-F, {seq(r[i], i=1..m)}, 'distributed'), [seq(r[i], i=1..m)])};
  sol := solve(C);

  if nops({sol})=0 then 
    return 0;
  else
    ind := indets({seq(rhs(sol[i]), i=1..N)});
    G := subs({seq(ind[i]=0, i=1..nops(ind))}, subs(sol, G));
    return expand(G);
  fi;
  
end:</Text-field>
</Input>
</Group>
