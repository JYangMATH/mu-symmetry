<Group hide-input="false" labelreference="L317" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">LSCgist := proc(F, mu)
##############################
#
# Input:  F, a mu-symmetric polynomial; mu, multiplicity structure
# Output: convert F into an expression in z[0], ...,z[n-1] by solving linear systems
#
##############################
  local n, m, delta, p,P, B, N, i, G, H, C, kv, ind, K, M;
 
  n   := add(mu);
  m   := nops(mu):
delta := degree(F, {seq(r[i], i=1..m)});
  if n&gt;delta then
    P := partition(delta):
  else
    P := partition(delta, n);
  fi;

  M   := CompleteHomoBasis(n);
  B   := Specialization(M, mu);
  N := 0;G := 0:
  for p in P do
    N := N+1;
    G := G + k[N]*product(z[p[i]], i=1..nops(p));
  od;

  H   := subs({seq(z[i]=B[i], i=1..n)}, G);
  C   := {coeffs(collect(H-F, {seq(r[i], i=1..m)}, 'distributed'), [seq(r[i], i=1..m)])};
  kv  := solve(C);

  if nops({kv})=0 then 
    return 0; 
  else 
    ind := indets({seq(rhs(kv[i]), i=1..N)});
    G := subs({seq(ind[i]=0, i=1..nops(ind))}, subs(kv, G));
#    M := Hp2Ep(n):
#    G := subs({seq(z[i]=M[i], i=1..n)}, G):
    return expand(G);
  fi;
end:
</Text-field>
</Input>
