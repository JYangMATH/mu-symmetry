<Section collapsed="true" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 2" layout="Heading 2">3.1 Check mu-symmetry with canonize+reduce</Text-field></Title>
<Group hide-input="false" labelreference="L245" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">isMuSymmetric := proc(F, mu)
# Check mu-symmetry of F using reduce and canonize algorithms

  local m, delta, n, e, P, B, p, R, i, t1, t2, t3;

t1 := time():
  m   := nops(mu):
delta := degree(F, {seq(r[i], i=1..m)}):
  n   := add(mu);
  e   := Specialization(genelsym(n), mu);
  if delta&gt;=n then 
    P := partition(delta,n);
  else
    P := partition(delta);
  fi;
  B := []:
  for p in P do
    B := [op(B), product(e[p[i]], i=1..nops(p))];
  od:
  
  
  B := expand(B):
  B := Canonize(B);
t2 := time():
  R := muReduce(F, B);
t3 := time():

  if R=0 then 
    return 1, [t2-t1, t3-t2, t3-t1]; 
  else 
    return 0, [t2-t1, t3-t2, t3-t1]; 
  fi;
end:


muReduce := proc(F, B)
# Compute the reduced polynomial of F w.r.t. B
  local m, l, i, k, q, f, lf, lb;
  
  m  := nops(indets(B)): 
  l  := nops(B);
  i  := l;
  q  := 0:
  f  := expand(F):

  while f&lt;&gt;0 and i&gt;0 do
    lf := LeadingTerm(f, plex(seq(r[m-k+1], k=1..m)));
    lb := LeadingTerm(B[i], plex(seq(r[m-k+1], k=1..m)));
    if lf[2]=lb[2] then
      f := expand(f-lf[1]/lb[1]*B[i]);
      i := i-1;
    elif LeadingTerm(lf[2]-lb[2], plex(seq(r[m-k+1], k=1..m)))=lf[2] then
      q := q + lf[1]*lf[2]:
      f := f - lf[1]*lf[2]:
    else
      i := i-1;
    fi;
  od:
  
  return q+f;
end:


Canonize := proc(Fs)
# Generate the canonical set of Fs

  local m, B, P, b, c, C;

  m := nops(indets(Fs));
  B := [];
  P := stack[new](op(Fs)):
  while stack[depth](P)&gt;0 do
    b := stack[pop](P);
    b := muReduce(b, B);
    if b&lt;&gt;0 then 
      B := polyInsert(b, B):
    fi;
  od;

  return B;
end:

polyInsert := proc(b, B)

  local m,p,lB,l,i,k:

  m  := nops(indets([b, op(B)]));
  p  := LeadingMonomial(b, plex(seq(r[m-k+1], k=1..m)));
  lB := LeadingMonomial(B, plex(seq(r[m-k+1], k=1..m)));
  l  := nops(B):
  if l=0 then return [b]; fi;

  for i from 1 to l do
    if LeadingMonomial(p-lB[i], plex(seq(r[m-k+1], k=1..m))) = lB[i] then
        return [op(1..i-1, B), b, op(i..l, B)]; 
    else 
      if i=l then return [op(B), b]; fi;
    fi;
  od;

end:</Text-field>
</Input>
</Group>
